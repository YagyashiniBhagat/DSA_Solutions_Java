class Solution {
    public double myPow(double x, int n) {
        int m=n;
        long p=n;
        if(p<0) p=-p;
        double ans=1.0;
        while(p>0){
            if(p%2==1){
                ans=ans*x;
                p=p-1;
            }
            else{
                x=x*x;
                p=p/2;
            }
        }
        return (n<0) ? 1.0/ans : ans;
    }
}
