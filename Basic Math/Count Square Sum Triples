class Solution {
    public int countTriples(int n) {
        int cnt=0;
        for(int a=1;a<=n;a++){
            for(int b=a+1;b<=n;b++){
                int c=(a*a)+(b*b);
                int x=(int)Math.sqrt(c);
                if((x*x)==c && x<=n) cnt+=2;
            }
        }
        return cnt;
    }
}
