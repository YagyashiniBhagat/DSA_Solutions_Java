class Solution {
    static void insertatbottom(Stack<Integer> st,int val){
        if(st.isEmpty()){
            st.push(val);
            return;
        }
        int t=st.pop();
        insertatbottom(st,val);
        st.push(t);
    }
    public static void reverseStack(Stack<Integer> st) {
        // code here
        if(st.isEmpty()) return ;
        int t=st.pop();
        reverseStack(st);
        insertatbottom(st,t);
    }
}
