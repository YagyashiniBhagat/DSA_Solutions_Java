class Solution {
    public void combination(int start,int k,int target,List<Integer> curr,List<List<Integer>> ans){
        if(curr.size()==k){
            if(target==0){
                ans.add(new ArrayList<>(curr));
                return;
            }
        }
        if(curr.size()>k || target<0) return;
        for(int i=start;i<=9;i++){
            curr.add(i);
            combination(i+1,k,target-i,curr,ans);
            curr.remove(curr.size()-1);
        }
    }
    public List<List<Integer>> combinationSum3(int k, int n) {
        List<List<Integer>> ans=new ArrayList<>();
        combination(1,k,n,new ArrayList<>(),ans);
        return ans;
    }
}
