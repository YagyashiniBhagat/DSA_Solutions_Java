class Solution {
    void generate(int n,String curr,ArrayList<String> ans){
        if(curr.length()==n){
            ans.add(curr);
            return;
        }
        generate(n,curr+"0",ans);
        generate(n,curr+"1",ans);
    }
    public ArrayList<String> binstr(int n) {
        // code here
        ArrayList<String> ans=new ArrayList<>();
        generate(n,"",ans);
        return ans;
    }
}

3211. Generate Binary Strings Without Adjacent Zeros
class Solution {
    void generate(int n,String curr,List<String> ans){
        if(curr.length()==n){
            ans.add(curr);
            return;
        }
        generate(n,curr+"1",ans);
        if(curr.isEmpty() || curr.charAt(curr.length()-1)!='0'){
            generate(n,curr+"0",ans);
        }
    }
    public List<String> validStrings(int n) {
        List<String> ans=new ArrayList<>();
        generate(n,"",ans);
        return ans;
    }
}
