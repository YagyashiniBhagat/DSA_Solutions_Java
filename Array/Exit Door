import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-- > 0){
            int n=sc.nextInt();
            List<Integer> arr=new ArrayList<>();
            for(int i=0;i<n;i++)
                arr.add(sc.nextInt());
            
            int disturbed=0;
            for(int person=n;person>=1;person--){
                int pos=arr.indexOf(person);
                int left=pos;
                int right=arr.size()-pos-1;
                disturbed+=Math.min(left,right);
                arr.remove(pos);
            }
            System.out.println(disturbed);
        }
	}
}
