class Solution {
    public int minOperations(int[] nums) {
        Deque<Integer> stack=new ArrayDeque<>();
        int opr=0;
        for(int i:nums){
            while(!stack.isEmpty() && stack.peek()>i){
                stack.pop();
            }
            if(i==0) continue;
            if(stack.isEmpty() || stack.peek()!=i){
                stack.push(i);
                opr++;
            }
        }
        return opr;
    }
}
