class Solution {
    public int[] minBitwiseArray(List<Integer> nums) {
        int n=nums.size();
        int[] ans=new int[n];
        for(int i=0;i<n;i++){
            if(nums.get(i)==2){
                ans[i]=-1;
                continue;
            }
            boolean found=false;
            ans[i]=-1;
            for(int j=0;j<nums.get(i);j++){
                if((j | (j+1)) == nums.get(i)){
                    ans[i]=j;
                    found=true;
                    break;
                }
            }
        }
        return ans;
    }
}
