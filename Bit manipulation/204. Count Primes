class Solution {
    public int countPrimes(int n) {
        if(n==0 || n==1 || n==2) return 0;
        int[] l=new int[n];
        for(int i=2;i<n;i++) l[i]=1;
        for(int i=2;i*i<n;i++){
            if(l[i]==1){
                for(int j=2*i;j<n;j+=i){
                    l[j]=0;
                }
            }
        }
        int c=0;
        for(int num:l){
            if(num==1) c++;
        }
        return c;
    }
}
